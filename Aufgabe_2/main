# JSON lesen
data = InputFileReader_modularisiert.read_json("Aufgabe_2/test.json")

# FDS schreiben
InputFileReader_modularisiert.write_fds(listOfMbsObjects, "Aufgabe_2/test.fds")

# Anzahl der Objekte ausgeben
print(len(listOfMbsObjects))